@font-face {
    font-family: 'Proxima Nova Rg';
    src: url('/fonts/ProximaNova-Regular.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

.breadCrumbs {
    font-family: "Proxima Nova Rg", sans-serif;
    font-size: 14px;
    font-weight: 400;
    line-height: 24px;
    letter-spacing: 0;
    text-align: left;
    color: rgba(136, 136, 136, 1);
    margin: 24px 0 0 0;
    
    a {
        text-decoration: none;
        color: inherit;
    }
}